#McKinsey Intranet account
Host githuben.intranet.mckinsey.com
    HostName githuben.intranet.mckinsey.com
    User git
    IdentityFile ~/.ssh/id_rsa_mck

#McKinsey Github account
Host github.com-work
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa_mck

#Personal Github account
Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa
